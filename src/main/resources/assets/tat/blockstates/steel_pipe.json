{
  "multipart": [
    {   "when": { "north": true },
      "apply": { "model": "tat:block/steel_pipe_side" }
    },
    {   "when": { "east": true },
      "apply": { "model": "tat:block/steel_pipe_side", "y": 90, "uvlock": true }
    },
    {   "when": { "south": true },
      "apply": { "model": "tat:block/steel_pipe_side", "y": 180, "uvlock": true }
    },
    {   "when": { "west": true },
      "apply": { "model": "tat:block/steel_pipe_side", "y": 270, "uvlock": true }
    },
    {   "when": { "up": true },
      "apply": { "model": "tat:block/steel_pipe_side", "x": 270, "uvlock": true }
    },
    {   "when": { "down": true },
      "apply": { "model": "tat:block/steel_pipe_side", "x": 90, "uvlock": true }
    },
    {   "when": { "north": false },
      "apply": [
        { "model": "tat:block/steel_pipe_noside", "weight": 2 },
        { "model": "tat:block/steel_pipe_noside" },
        { "model": "tat:block/steel_pipe_noside" },
        { "model": "tat:block/steel_pipe_noside" }
      ]
    },
    {   "when": { "east": false },
      "apply": [
        { "model": "tat:block/steel_pipe_noside", "y": 90, "uvlock": true },
        { "model": "tat:block/steel_pipe_noside", "y": 90, "uvlock": true },
        { "model": "tat:block/steel_pipe_noside", "y": 90, "uvlock": true },
        { "model": "tat:block/steel_pipe_noside", "weight": 2, "y": 90, "uvlock": true }
      ]
    },
    {   "when": { "south": false },
      "apply": [
        { "model": "tat:block/steel_pipe_noside", "y": 180, "uvlock": true },
        { "model": "tat:block/steel_pipe_noside", "y": 180, "uvlock": true },
        { "model": "tat:block/steel_pipe_noside", "weight": 2, "y": 180, "uvlock": true },
        { "model": "tat:block/steel_pipe_noside", "y": 180, "uvlock": true }
      ]
    },
    {   "when": { "west": false },
      "apply": [
        { "model": "tat:block/steel_pipe_noside", "y": 270, "uvlock": true },
        { "model": "tat:block/steel_pipe_noside", "weight": 2, "y": 270, "uvlock": true },
        { "model": "tat:block/steel_pipe_noside", "y": 270, "uvlock": true },
        { "model": "tat:block/steel_pipe_noside", "y": 270, "uvlock": true }
      ]
    },
    {   "when": { "up": false },
      "apply": [
        { "model": "tat:block/steel_pipe_noside", "weight": 2, "x": 270, "uvlock": true },
        { "model": "tat:block/steel_pipe_noside", "x": 270, "uvlock": true },
        { "model": "tat:block/steel_pipe_noside", "x": 270, "uvlock": true },
        { "model": "tat:block/steel_pipe_noside", "x": 270, "uvlock": true }
      ]
    },
    {   "when": { "down": false },
      "apply": [
        { "model": "tat:block/steel_pipe_noside", "x": 90, "uvlock": true },
        { "model": "tat:block/steel_pipe_noside", "x": 90, "uvlock": true },
        { "model": "tat:block/steel_pipe_noside", "x": 90, "uvlock": true },
        { "model": "tat:block/steel_pipe_noside", "weight": 2, "x": 90, "uvlock": true }
      ]
    }
  ]
}
